{
    "name": "trensy/framework",
    "description": "a fast php framework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Wang",
            "email": "hpuwang@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.30",
        "ext-swoole" : ">=1.8.*",
        "ext-mbstring": "*",
        "ext-posix":"*"
    },
    "replace":{

    },
    "bin": [
        "trensy"
    ],
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Trensy\\": "src/trensy"
        },
        "files":[
            "src/trensy/Support/Predis.php"
        ]
    },
    "suggest": {
        "ext-msgpack": "data serialize tool",
        "ext-igbinary":"data serialize tool",
        "ext-hprose":"data serialize tool",
        "ext-apc":"php opcode cache , not support7",
        "ext-apcu":"php7 support "
    },
    "autoload-dev": {
        "psr-4": {
            "Trensy\\Test\\": "tests/application/src"
        }
    },
    "minimum-stability": "dev",
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
