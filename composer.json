{
    "name": "trendi/framework",
    "description": "a fast php framework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Wang",
            "email": "hpuwang@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.30",
        "ext-swoole" : "1.8.*",
        "ext-mbstring": "*",
        "ext-posix":"*",
        "classpreloader/classpreloader": "~3.0",
        "monolog/monolog": "~1.11",
        "mtdowling/cron-expression": "~1.1",
        "swiftmailer/swiftmailer": "^5.4",
        "symfony/console": "3.1.*",
        "symfony/config":"3.1.*",
        "symfony/http-foundation": "3.1.*",
        "symfony/http-kernel": "3.1.*",
        "symfony/process": "3.1.*",
        "symfony/routing": "3.1.*",
        "symfony/translation": "3.1.*",
        "symfony/var-dumper": "3.1.*",
        "symfony/dependency-injection":"3.1.*",
        "symfony/expression-language":"3.1.*",
        "symfony/proxy-manager-bridge":"3.1.*",
        "jeremeamia/superclosure": "^2.2",
        "illuminate/view": "^5.3",
        "predis/predis": "^1.1"
    },
    "replace":{

    },
    "require-dev": {
        "phpunit/phpunit": "~5.4"
    },
    "autoload": {
        "psr-4": {
            "Trendi\\": "src/trendi"
        }
    },
    "suggest": {
        "ext-msgpack": "data serialize tool",
        "ext-igbinary":"data serialize tool",
        "ext-hprose":"data serialize tool"
    },
    "autoload-dev": {
        "psr-4": {
            "Trendi\\Test\\": "tests/application/src"
        }
    },
    "minimum-stability": "dev",
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
