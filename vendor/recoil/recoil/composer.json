{
    "name": "recoil/recoil",
    "description": "A generator-based cooperative multitasking kernel for React.",
    "require": {
        "php": "^7",
        "icecave/repr": "^1",
        "react/event-loop": "^0.4",
        "react/promise": "^2"
    },
    "require-dev": {
        "ext-posix": "*",
        "eloquent/phony": "^0.13",
        "friendsofphp/php-cs-fixer": "^1",
        "hamcrest/hamcrest-php": "^2",
        "peridot-php/leo": "^1",
        "peridot-php/peridot": "^1",
        "peridot-php/peridot-code-coverage-reporters": "^2",
        "react/dns": "^0.4"
    },
    "autoload": { "psr-4": { "Recoil\\": "src" } },
    "autoload-dev": {
        "psr-4": { "Recoil\\": "test/src" },
        "files": [ "test/src/functional-tests.php" ]
    },
    "keywords": [
        "coroutine",
        "generator",
        "event",
        "async",
        "asynchronous",
        "react",
        "green",
        "thread",
        "multitasking",
        "continuation"
    ],
    "homepage": "https://github.com/recoilphp/recoil",
    "license": "MIT",
    "authors": [
        {
            "name": "James Harris",
            "email": "james.harris@icecave.com.au",
            "homepage": "https://github.com/jmalloc"
        }
    ],
    "config": {
        "sort-packages": true
    }
}
